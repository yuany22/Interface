package com.chuange.aishijing.vo.servicevo.classes;

/**
 * Created by Administrator on 2018-12-10.
 */
public class ClassesRequest {
    private String smallChapterTitle;
    private String classtitle;
    private String teacher;
    private String price;
    private String castIntroduce;
    private String bigChapterTitle;
    private String courseUrl;
    private String videoUrl;
    public ClassesRequest(){}

    public ClassesRequest(String smallChapterTitle, String classtitle, String teacher, String price, String castIntroduce, String bigChapterTitle, String courseUrl, String videoUrl) {
        this.smallChapterTitle = smallChapterTitle;
        this.classtitle = classtitle;
        this.teacher = teacher;
        this.price = price;
        this.castIntroduce = castIntroduce;
        this.bigChapterTitle = bigChapterTitle;
        this.courseUrl = courseUrl;
        this.videoUrl = videoUrl;
    }

    @Override
    public String toString() {
        return "ClassesRequest{" +
                "smallChapterTitle='" + smallChapterTitle + '\'' +
                ", classtitle='" + classtitle + '\'' +
                ", teacher='" + teacher + '\'' +
                ", price='" + price + '\'' +
                ", castIntroduce='" + castIntroduce + '\'' +
                ", bigChapterTitle='" + bigChapterTitle + '\'' +
                ", courseUrl='" + courseUrl + '\'' +
                ", videoUrl='" + videoUrl + '\'' +
                '}';
    }

    public String getSmallChapterTitle() {
        return smallChapterTitle;
    }

    public void setSmallChapterTitle(String smallChapterTitle) {
        this.smallChapterTitle = smallChapterTitle;
    }

    public String getClasstitle() {
        return classtitle;
    }

    public void setClasstitle(String classtitle) {
        this.classtitle = classtitle;
    }

    public String getTeacher() {
        return teacher;
    }

    public void setTeacher(String teacher) {
        this.teacher = teacher;
    }

    public String getPrice() {
        return price;
    }

    public void setPrice(String price) {
        this.price = price;
    }

    public String getCastIntroduce() {
        return castIntroduce;
    }

    public void setCastIntroduce(String castIntroduce) {
        this.castIntroduce = castIntroduce;
    }

    public String getBigChapterTitle() {
        return bigChapterTitle;
    }

    public void setBigChapterTitle(String bigChapterTitle) {
        this.bigChapterTitle = bigChapterTitle;
    }

    public String getCourseUrl() {
        return courseUrl;
    }

    public void setCourseUrl(String courseUrl) {
        this.courseUrl = courseUrl;
    }

    public String getVideoUrl() {
        return videoUrl;
    }

    public void setVideoUrl(String videoUrl) {
        this.videoUrl = videoUrl;
    }
}
